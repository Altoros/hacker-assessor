<p id="notice"><%= notice %></p>

<h1>Listing Acquirements</h1>

<table>
  <thead>
    <tr>
      <th>Level</th>
      <th>Hacker</th>
      <th>Skill</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @acquirements.each do |acquirement| %>
      <tr>
        <td><%= acquirement.level %></td>
        <td><%= acquirement.hacker %></td>
        <td><%= acquirement.skill %></td>
        <td><%= link_to 'Show', acquirement %></td>
        <td><%= link_to 'Edit', edit_acquirement_path(acquirement) %></td>
        <td><%= link_to 'Destroy', acquirement, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Acquirement', new_acquirement_path %>
