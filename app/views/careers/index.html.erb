<h1>Listing Careers</h1>
<%= link_to 'New Career', new_career_path %>
<ul class="careers">
  <% @careers.each do |career| %>
    <li>
      <%= link_to career do %>
        <h2><%= career.name %></h2>
      <% end %>
      <%= career.description %>
      <%= link_to 'Panorama', panorama_career_path(career) %>
    </li>
  <% end %>
</ul>
